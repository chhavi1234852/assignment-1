#include <iostream>
using namespace std;

int main() {
    int arr[100], n;
    cout << "Enter number of elements: ";
    cin >> n;

    cout << "Enter elements:\n";
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    cout << "Reversed array: ";
    for (int i = n - 1; i >= 0; i--)
        cout << arr[i] << " ";
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int a[10][10], b[10][10], c[10][10];
    int n, m, p, q;

    cout << "Enter rows and columns of first matrix: ";
    cin >> n >> m;

    cout << "Enter elements of first matrix:\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> a[i][j];

    cout << "Enter rows and columns of second matrix: ";
    cin >> p >> q;

    if (m != p) {
        cout << "Matrix multiplication not possible.\n";
        return 0;
    }

    cout << "Enter elements of second matrix:\n";
    for (int i = 0; i < p; i++)
        for (int j = 0; j < q; j++)
            cin >> b[i][j];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < q; j++) {
            c[i][j] = 0;
            for (int k = 0; k < m; k++)
                c[i][j] += a[i][k] * b[k][j];
        }

    cout << "Resultant Matrix:\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < q; j++)
            cout << c[i][j] << " ";
        cout << endl;
    }

    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int a[10][10], trans[10][10], n, m;
    cout << "Enter rows and columns: ";
    cin >> n >> m;

    cout << "Enter elements:\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> a[i][j];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            trans[j][i] = a[i][j];

    cout << "Transpose of the matrix:\n";
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++)
            cout << trans[i][j] << " ";
        cout << endl;
    }

    return 0;
}

